{
    "import": {
        "prefix": "im",
        "description": "Import statement",
        "body": "import ${variableName} from '${path}';"
    },
    "require": {
        "prefix": "re",
        "description": "Require statement",
        "body": "require('${path}');"
    },
    "let": {
        "prefix": "let",
        "description": "let variable",
        "body": "let ${variableName} = ${value};"
    },
    "constant": {
        "prefix": "const",
        "description": "constant variable",
        "body": "const ${VARIABLE_NAME} = ${value};"
    },
    "class": {
        "prefix": "class",
        "description": "Class with empty body",
        "body": [
            "class ${ComponentName} {",
            "\t$0",
            "}"
        ]
    },
    "class with constructor": {
        "prefix": "classc",
        "description": "Class with constructor",
        "body": [
            "class ${ComponentName} {",
            "\tconstructor(${parameters}) {",
            "\t\t$0",
            "\t}",
            "}"
        ]
    },
    "class that extends another class": {
        "prefix": "classe",
        "description": "Class that extends another class",
        "body": [
            "class ${ComponentName} extends ${AnotherClassName} {",
            "\tconstructor(${parameters}) {",
            "\t\tsuper(${parameters});",
            "\t\t$0",
            "\t}",
            "}"
        ]
    },
    "class constructor": {
        "prefix": "ctor",
        "description": "Class constructor",
        "body": [
            "constructor(${parameters}) {",
            "\t$0",
            "}"
        ]
    },
    "function": {
        "prefix": "fn",
        "description": "Function",
        "body": [
            "function ${name}(${parameters}) {",
            "\t$0",
            "}"
        ]
    },
    "anonymous function": {
        "prefix": "fn",
        "description": "Anonymous function",
        "body": [
            "function (${parameters}) {",
            "\t$0",
            "}"
        ]
    },
    "switch": {
        "prefix": "sw",
        "description": "Switch",
        "body": [
            "switch (${variable}) {",
            "\tcase ${value}:",
            "\t\t$0",
            "\t\tbreak;",
            "\tdefault:",
            "\t\t$1",
            "\t\treturn;",
            "}"
        ]
    },
    "console log": {
        "prefix": "cl",
        "description": "console.log() statement",
        "body": "console.log(${value});"
    },
    "debugger": {
        "prefix": "de",
        "description": "debugger statement",
        "body": "debugger;"
    }
}